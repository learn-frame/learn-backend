{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json",
    "assets": [
      "**/*.proto"
    ],
    "watchAssets": true,
    "builder": "swc",
    "typeCheck": true
  },
  "monorepo": true,
  "root": "apps/gateway",
  "projects": {
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "product": {
      "type": "application",
      "root": "apps/product",
      "entryFile": "main",
      "sourceRoot": "apps/product/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product/tsconfig.app.json"
      }
    },
    "__playground__": {
      "type": "application",
      "root": "apps/__playground__",
      "entryFile": "main",
      "sourceRoot": "apps/__playground__/src",
      "compilerOptions": {
        "tsConfigPath": "apps/__playground__/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "order": {
      "type": "application",
      "root": "apps/order",
      "entryFile": "main",
      "sourceRoot": "apps/order/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order/tsconfig.app.json"
      }
    },
    "cart": {
      "type": "application",
      "root": "apps/cart",
      "entryFile": "main",
      "sourceRoot": "apps/cart/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cart/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "constant": {
      "type": "library",
      "root": "libs/constant",
      "entryFile": "index",
      "sourceRoot": "libs/constant/src",
      "compilerOptions": {
        "tsConfigPath": "libs/constant/tsconfig.lib.json"
      }
    },
    "etcd": {
      "type": "library",
      "root": "libs/etcd",
      "entryFile": "index",
      "sourceRoot": "libs/etcd/src",
      "compilerOptions": {
        "tsConfigPath": "libs/etcd/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "mysql": {
      "type": "library",
      "root": "libs/mysql",
      "entryFile": "index",
      "sourceRoot": "libs/mysql/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mysql/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "rabbit-mq": {
      "type": "library",
      "root": "libs/rabbit-mq",
      "entryFile": "index",
      "sourceRoot": "libs/rabbit-mq/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rabbit-mq/tsconfig.lib.json"
      }
    },
    "middleware": {
      "type": "library",
      "root": "libs/middleware",
      "entryFile": "index",
      "sourceRoot": "libs/middleware/src",
      "compilerOptions": {
        "tsConfigPath": "libs/middleware/tsconfig.lib.json"
      }
    }
  }
}