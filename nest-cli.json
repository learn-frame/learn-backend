{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json",
    "assets": [
      "**/*.proto"
    ],
    "watchAssets": true,
    "builder": "swc"
  },
  "monorepo": true,
  "root": "apps/gateway",
  "projects": {
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "product": {
      "type": "application",
      "root": "apps/product",
      "entryFile": "main",
      "sourceRoot": "apps/product/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product/tsconfig.app.json"
      }
    },
    "__playground__": {
      "type": "application",
      "root": "apps/__playground__",
      "entryFile": "main",
      "sourceRoot": "apps/__playground__/src",
      "compilerOptions": {
        "tsConfigPath": "apps/__playground__/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "order": {
      "type": "application",
      "root": "apps/order",
      "entryFile": "main",
      "sourceRoot": "apps/order/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order/tsconfig.app.json"
      }
    },
    "cart": {
      "type": "application",
      "root": "apps/cart",
      "entryFile": "main",
      "sourceRoot": "apps/cart/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cart/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "middileware": {
      "type": "library",
      "root": "libs/middileware",
      "entryFile": "index",
      "sourceRoot": "libs/middileware/src",
      "compilerOptions": {
        "tsConfigPath": "libs/middileware/tsconfig.lib.json"
      }
    },
    "constant": {
      "type": "library",
      "root": "libs/constant",
      "entryFile": "index",
      "sourceRoot": "libs/constant/src",
      "compilerOptions": {
        "tsConfigPath": "libs/constant/tsconfig.lib.json"
      }
    }
  }
}