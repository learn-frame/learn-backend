{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/common": [
        "libs/common/src"
      ],
      "@app/common/*": [
        "libs/common/src/*"
      ],
      "@app/logger": [
        "libs/logger/src"
      ],
      "@app/logger/*": [
        "libs/logger/src/*"
      ],
      "@app/middileware": [
        "libs/middileware/src"
      ],
      "@app/middileware/*": [
        "libs/middileware/src/*"
      ],
      "@app/constant": [
        "libs/constant/src"
      ],
      "@app/constant/*": [
        "libs/constant/src/*"
      ],
      "@app/etcd": [
        "libs/etcd/src"
      ],
      "@app/etcd/*": [
        "libs/etcd/src/*"
      ],
      "@app/redis": [
        "libs/redis/src"
      ],
      "@app/redis/*": [
        "libs/redis/src/*"
      ],
      "@app/rabbitmq": [
        "libs/rabbitmq/src"
      ],
      "@app/rabbitmq/*": [
        "libs/rabbitmq/src/*"
      ],
      "@app/mysql": [
        "libs/mysql/src"
      ],
      "@app/mysql/*": [
        "libs/mysql/src/*"
      ],
      "@app/auth": [
        "libs/auth/src"
      ],
      "@app/auth/*": [
        "libs/auth/src/*"
      ],
      "@app/rabbit-mq": [
        "libs/rabbit-mq/src"
      ],
      "@app/rabbit-mq/*": [
        "libs/rabbit-mq/src/*"
      ],
      "@app/middleware": [
        "libs/middleware/src"
      ],
      "@app/middleware/*": [
        "libs/middleware/src/*"
      ]
    }
  }
}